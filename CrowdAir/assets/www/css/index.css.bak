* {
	-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
	/* make transparent link selection, adjust last value opacity 0 to 1.0 */
}

html,body {
	font-family: 'HelveticaNeue-Light', 'HelveticaNeue', Helvetica, Arial,
		sans-serif;
	font-size: 20px;
	margin: 0 auto;
	padding: 0px;
	min-height: 100%;
	height: 100%;
	width: 100%;
}



table {
	font-size: 15px;
	padding: 5px;
	overflow: hidden;
	text-align: left;
	width: 100%;
}

a {
	text-decoration: none;
	color: #ffffff;
}
/* Portrait layout (default) */

/* Landscape layout (with min-width) */
@media screen and (min-aspect-ratio: 1/1) and (min-width:400px) {
}

.wrapper {
	position: relative;
	width: 100%;
	margin: 0 auto;
	overflow: hidden;
	min-height: 100%;
	height: 100%;
}

.startpage-login{
	height: 100%;
	width: 100%;
	text-align: center;
	position: relative;
	background-color: #efeff3;
}

.startpage-fourblocks {
	/* @yingzi: This is for the index.html starting page */
	height: 90%;
	width: 100%;
	text-align: center;
	position: relative;
	background-color: #efeff3;
}

.startpage-fourblocks .block {
	height: 25%;
	cursor: pointer;
	overflow: hidden;
}

.startpage-fourblocks .block div {
	position: relative;
	top: 40%;
}

.startpage-threeblocks {
	/* @yingzi: This is for the index.html starting page */
	height: 100%;
	width: 100%;
	text-align: center;
	position: relative;
	background-color: #efeff3;
}

.startpage-threeblocks .block {
	height: 25%;
	cursor: pointer;
	overflow: hidden;
}

.startpage-threeblocks .block div {
	position: relative;
	top: 40%;
}

.line-separator {
	border-bottom: 2px solid #4ebfcc;
	width: 100%;
}

.startpage-threeblocks .block a {
	
}

.startpage-bottom {
	height: 9%;
	width: 100%;
	display: table;
	text-align: center;
	background-color: #000000;
	/* overflow: scroll;
	position: relative; */
}

.startpage-bottom a {
	display: table-cell;
	vertical-align: middle;
}

.metrotemp {
	height: 40%;
	position: relative;
}

.metrotemp div {
	position: relative;
	top: 40%;
}

.metrolist {
	display: table;
	width: 100%;
	height: 20%;
}

.metro3days {
	height: 40%;
}

.metro3days table tr td:first-child {
	padding-right: 50px;
}

.aqi-header {
	height: 30%;
}

.aqi-map {
	height: 70%;
}

.pollen-header {
	height: 60%;
}

.pollen-content {
	height: 40%;
}

.arrow {
	float: right;
	display: inline-block;
	margin-right: 40px;
}

.arrow img {
	vertical-align: middle;
}

.clear {
	clear: both;
}

.text {
	display: inline-block;
}

.arrow-left {
	float: left;
	left: 10px;
	position: relative !important;
	top: 10% !important;
}

.pollen-table {
	text-align: center !important;
	margin-top: 20px;
}

.pollen-table tr td {
	padding-bottom: 5px;
	border: 1px solid #ffffff;
	background-color: #ffffff;
	color: #edbd5e;
}

.track-bottom {
	width: 100%;
	background: url("../img/back1.png");
	position: fixed;
	bottom: 10%;
	left: 0;
	height: 100px;
	z-index: 100;
}

.two-buttons {
	text-align: center;
	height: 40px;
	display: block;
}

.two-buttons a {
	margin-left: 35px;
	border: 1px solid #ffffff;
	width: 100px;
	display: inline-block;
	line-height: 2;
	font-size: 15px;
	font-weight: bold;
}

.two-buttons a:first-child {
	margin-left: 0;
}

.trackstrat-bottom {
	width: 100%;
	background: url("../img/back1.png") repeat-x;
	left: 0;
	height: 180px;
	z-index: 10;
	position: fixed;
	bottom: 10%;
	left: 0;
}

.tracttext {
	font-size: 15px;
	text-align: left;
	margin-left: 10px;
	color: #ffffff;
	margin-top: 10px;
	margin-bottom: 20px;
}

.three-href {
	text-align: center;
	height: 40px;
}

.three-href a {
	display: inline-block;
	line-height: 2;
	margin-left: 20px;
	font-size: 15px;
	font-weight: bold;
}

.three-href a:first-child {
	width: 100px;
	border: 1px solid #ffffff;
}

.three-href a img {
	vertical-align: middle;
}

ul {
	list-style: none;
	padding: 0;
	max-height: 90%;
	margin-top: 0;
}

ul li {
	padding: 5px;
	overflow: hidden;
	line-height: 1;
	border-bottom: 3px solid #d7d7d7;
}

.datetime {
	text-align: left;
	float: left;
}

.checkbox{
	text-align: left;
	float: left;
	vertical-align: center;
	height: 50px; 
	line-height:50px; 
	
}

