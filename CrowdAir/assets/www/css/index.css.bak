	font:size="5";
	color:"blue";
} */

.entireLine {
	text-align: left;
	float: none;
}

.textarea {
	text-align: left;
	float: left;
	width: 100%;
}

.symptom-choice {
	text-align: right;
	vertical-align: bottom;
	width: 200px;
}

.symptom-bottom {
	background: url(../img/symptom-bottomback.png) repeat-x;
	height: 160px;
	position: absolute;
	bottom: 0;
	width: 100%;
}

.smallarrow-right {
	background: url(../img/small-arrowright.png) no-repeat center;
	float: right;
}

.symheart-blank {
	display: inline-block;
	background: url(../img/heart-blank.png) no-repeat center;
	height: 25px;
	width: 29px;
	margin-left: 20px;
}

.symheart-blank:first-child {
	margin-left: 0;
}

.sysmheart {
	display: inline-block;
	background: url(../img/heart.png) no-repeat center;
	height: 25px;
	width: 29px;
	margin-left: 20px;
}

.sysmheart:first-child {
	margin-left: 0;
}

ul li a {
	background: url(../img/small-arrowright.png) no-repeat center right;
	display: block;
	padding-left: 20px;
	text-align: left;
	color: #ffffff;
	line-height: 27px;
	line-height: 1.5;
}

.symptom-bottom ul {
	margin: 0 auto;
	height: 160px;
}

.symptom-bottom ul li {
	height: 27px;
}

.symptom-bottom ul li:last-child {
	border: none;
}

input[type="radio"] {
	margin: 0 14px;
	display: inline-block;
	width: 19px;
	height: 25px;
	
}

.radio-label {
	padding-bottom: 10px;
}

.radio-label label {
	display: inline-block;
	margin: 0 18px;
}

.button-save-radio {
	text-align: right;
	bottom: 40PX;
	right: 20PX;
}

.button-save {
	text-align: right;
	position: absolute;
	bottom: 40PX;
	right: 20PX;
}
.pure-button {
	font-family: inherit;
	font-size: 100%;
	*font-size: 90%;
	*overflow: visible;
	padding: .5em 1em;
	color: #444;
	color: rgba(0, 0, 0, .8);
	*color: #444;
	border: 1px solid #999;
	border: 0 rgba(0, 0, 0, 0);
	background-color: #E6E6E6;
	text-decoration: none;
	border-radius: 2px
}

.pure-button-hidden {
	display: none
}

.pure-button::-moz-focus-inner {
	padding: 0;
	border: 0
}

.pure-button-primary,.pure-button-selected,a.pure-button-primary,a.pure-button-selected
	{
	background-color: #0078e7;
	color: #fff
}

.button-next {
	text-align: right;
	position: absolute;
	bottom: 40PX;
	right: 20PX;
}

.button-camera {
	text-align: center;
	position: absolute;
	bottom: 40PX;
	left: 40PX;
}

.button-save a {
	text-align: center;
	border: 1px solid #000000;
	padding: 5px 20px;
	color: #000000;
}

.button-next a {
	text-align: center;
	border: 1px solid #000000;
	padding: 5px 20px;
	color: #000000;
}

.setting ul li a {
	color: #000000;
	background: url(../img/arrow-rightblack.png) no-repeat center right;
}

table.health {
	margin: 20px auto 0;
	border-spacing: 0;
}

table.health tr td {
	text-align: left;
	border: solid 1px #000000;
	padding: 10px;
}

.circle img {
	vertical-align: middle;
}

table.health tr td:first-child {
	text-align: center;
}

.sensor {
	height: 25px;
	width: 200px;
	color: #bcbcbc;
	border: none;
	border: 2px solid #bcbcbc;
	margin-top: 20px;
}

.sensor:focus {
	color: #474747;
}

.citilink a {
	background-color: #000000;
	padding: 5px 40px;
	max-width: 500px;
	color: #ffffff;
	text-align: center;
}

.container {
  overflow:scroll; 
  margin: 0px auto;
  width: 100%;
  height:100%;
  position: relative;
}
.login {
	
  position: relative;
  top: 5%;
  margin: 80 auto;
  padding: 20px 20px 20px ;
  width: 80%;
  height:60%;
  background: white;
  border-radius: 3px;

  &:before {
    content: '';
    position: relative;
    top: -8px; right: -8px; bottom: -8px; left: -8px;
    z-index: -1;
    background: rgba(black, .08);
    border-radius: 4px;
  }

  h1 {
    margin: -20px -20px 21px;
    position: relative;
    top:5%;
    line-height: 40px;
    font-size: 30px;
    font-weight: bold;
    color: #555;
    text-align: center;
    text-shadow: 0 1px white;
    background: #f3f3f3;
    border-bottom: 1px solid #cfcfcf;
    border-radius: 3px 3px 0 0;
  }

/*   p { margin: 20px 0 0; }
  p:first-child { margin-top: 0; } */

  input[type=text], input[type=password] { width: 278px; }

  p.remember_me {
    float: left;
    line-height: 31px;

    label {
      font-size: 12px;
      color: #777;
      cursor: pointer;
    }

    input {
      position: relative;
      bottom: 1px;
      margin-right: 4px;
      vertical-align: middle;
    }
  }

 /*  p.submit { text-align: right; } */
}

.start-description {
  margin: 20px 0;
  font-size: 13px;
  color: #555;
  text-align: center;
  text-shadow: 0 1px #2a85a1;
  position: relative;
  a {
    color: #cce7fa;
    text-decoration: none;

    &:hover { text-decoration: underline; }
  }
}

:-moz-placeholder {
  color: #c9c9c9 !important;
  font-size: 13px;
}

::-webkit-input-placeholder {
  color: #ccc;
  font-size: 13px;
}

input {
  font-family: 'Lucida Grande', Tahoma, Verdana, sans-serif;
  font-size: 14px;
}

input[type=text], input[type=password] {
  margin: 5px;
  padding: 0 10px;
  overflow:scroll;
  width: 200px;
  height: 34px;
  color: #404040;
  background: white;
  border: 1px solid;
  border-color: #c4c4c4 #d1d1d1 #d4d4d4;
  border-radius: 2px;
  outline: 5px solid #eff4f7;
  -moz-outline-radius: 3px; // Can we get this on WebKit please?

  &:focus {
    border-color: #7dc9e2;
    outline-color: #dceefc;
    outline-offset: 0; // WebKit sets this to -1 by default
  }
}


.lt-ie9 {
  input[type=text], input[type=password] { line-height: 34px; }
}